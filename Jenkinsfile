pipeline {
    agent any

// dont need to include checkout stage as pipeline is pointing to SCM toretreive jenkinsfile

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                sh '  '
            }
        }
    }
        stage('Test') {
            steps {
                echo 'Testing...'
                sh ' '
            }
        }

        stage('Deliver/deploy??') {
            steps {
                echo 'Deploying..'
                sh ' '
            }
        }
}
